@incollection{Melcher.2020,
 abstract = {Der Beitrag stellt die Frage, welche Bedeutung politisches Wissen f{\"u}r die politische Legitimit{\"a}t konkreter Aspekte und die generalisierte politische Unterst{\"u}tzung von Parteien und Wahlen hat. Die Ergebnisse zeigen dreierlei. Erstens verweisen die Ergebnisse darauf, dass die B{\"u}rgerinnen konkrete Beurteilungen im Sinne eines Saldos zu einem generalisierten Urteil verdichten. Zweitens zeigen sie, dass politisches Wissen nach den spezifischen Beurteilungen von Legitimit{\"a}t den zweitst{\"a}rksten Einfluss auf die generalisierte Unterst{\"u}tzung von Parteien und Wahlen hat. Drittens schlie{\ss}lich ergeben sich zwar Unterschiede nach Alter, Geschlecht, Bildung und Ost-West, sie sind aber inhaltlich wenig bedeutsam. F{\"u}r die Zukunft der Demokratie verweisen die Ergebnisse darauf, dass politisches Wissen im positiven Sinne dazu beitr{\"a}gt, dass spezifische Urteile in positiver Weise in generalisierte Unterst{\"u}tzung {\"u}berf{\"u}hrt werden. Generalisierte Unterst{\"u}tzung ist der Puffer, den die Demokratie gegen{\"u}ber den allt{\"a}glichen Unbilden des politischen Prozesses braucht. F{\"u}r die Persistenz der Demokratie und ihre Abwehrf{\"a}higkeit gegen{\"u}ber ihren Kritikern ist es entscheidend, dass dieser Puffer aufgebaut wird. Politisches Wissen leistet dazu einen wichtigen Beitrag.},
 author = {Melcher, Reinhold and We{\ss}els, Bernhard},
 title = {Politische Kultur und politisches Wissen},
 pages = {59--78},
 publisher = {{Springer Fachmedien Wiesbaden}},
 isbn = {978-3-658-29558-5},
 editor = {Kneip, Sascha and Merkel, Wolfgang and We{\ss}els, Bernhard},
 booktitle = {Legitimit{\"a}tsprobleme: Zur Lage der Demokratie in Deutschland},
 year = {2020},
 address = {Wiesbaden},
 doi = {10.1007/978-3-658-29558-5{\textunderscore }3}
}


